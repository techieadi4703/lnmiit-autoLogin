{
  "manifest_version": 3,
  "name": "Auto Login Extension",
  "version": "1.0",
  "description": "Autofill and login to multiple sites automatically",
  "permissions": ["storage", "scripting"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["config.json"],
      "matches": ["<all_urls>"]
    }
  ]
}
